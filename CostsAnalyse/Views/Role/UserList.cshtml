
@{
    ViewData["Title"] = "List of " + ViewData["Role"];
}
@model List<UserApp> 
<h2>UserList</h2>
<table>
    @foreach (var m in Model)
    {
    <tr> 
        <td>@m.UserName</td>
        <td>@m.Year</td>
        <td>@m.Email</td> 
        <td>    <a asp-action="@ViewData["DeleteAction"]" asp-route-id="@m.Id">Delete</a> |</td>
    </tr>
    }
    </table>

